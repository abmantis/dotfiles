# Swaybar Command
    set $swaybar_cmd waybar

# Screen lock and off idle time
    set $screenidletimelock 600
    set $screenidletimeoff 660

# Background
    set $background ~/Pictures/Wallpapers/forest.jpg

# Theme
    set $gtk_theme 'Yaru-dark'

# Touchpad name
    set $touchpad_name "1739:31251:DLL07BE:01_06CB:7A13_Touchpad"    

# Autostart apps
    exec sleep 1 && gammastep
    exec sleep 5 && dropbox
    exec ~/Scripts/delayed_autostart_list.sh
    #exec pulseeffects --gapplication-service
