    set $ws6output  "Dell Inc. DELL U2417H 5K9YD89MC9FL"
    set $ws7output  "Dell Inc. DELL U2417H 5K9YD89MF92L"
    set $ws8output  "Dell Inc. DELL U2417H 5K9YD89MF92L"
    set $ws9output  "Dell Inc. DELL U2417H 5K9YD89MF92L"
